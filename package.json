{
  "name": "app-inventario-mobile",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "set NODE_TLS_REJECT_UNAUTHORIZED=0 && ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "build:mobile": "ng build --configuration mobile",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:app-inventario": "set NODE_TLS_REJECT_UNAUTHORIZED=0 && node dist/app-inventario/server/server.mjs",
    "cap:add": "npx cap add android",
    "cap:copy": "npx cap copy android",
    "cap:sync": "npx cap sync android",
    "cap:open": "npx cap open android",
    "cap:run": "npx cap run android",
    "cap:build": "npm run build:mobile && npx cap sync android",
    "mobile:dev": "npm run build:mobile && npx cap sync android && npx cap open android"
  },
  "private": true,
